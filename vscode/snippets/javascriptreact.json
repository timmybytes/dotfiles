  "Fetch Template": {
    "prefix": ["fetch", "fetem"],
    "body": ["fetch('${2:url}').then(res =>",
        "\tres.json().then(data => {",
          "\t\t// Do something with the data",
          "\t\t// e.g., setState(data)",
          "\t\t${1:console.log(data);}\n\t\t${0}",
        "\t})",
    ")"],
    "description": "A for loop."
  },
  "Arrow Function Component" : {
    "prefix": ["raff", "rarrow"],
    "body": ["import React from 'react'\n",
      "const ${2:Name} = () => {",
        "\treturn (",
          "\t\t<>",
          "\t\t</>",
        "\t)",
      "}\n",
      "export default ${2:Name};"],
      "description": "React Arrow Func Component"
  },
